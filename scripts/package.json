{
  "name": "@codebuff/scripts",
  "version": "1.0.0",
  "description": "Scripts package for Codebuff",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit -p .",
    "test": "bun test",
    "debug-log-subset": "bun run debug-log-subset.ts",
    "typecheck-all": "bun run typecheck-all.ts",
    "seed-agents": "bun run seed-default-agents.ts",
    "seed-agent-store": "bun run seed-agent-store.ts",
    "update-upstream": "bun run update-from-upstream.ts"
  },
  "sideEffects": false,
  "engines": {
    "bun": "^1.3.5"
  },
  "dependencies": {
    "@ai-sdk/openai-compatible": "^1.0.19",
    "@codebuff/bigquery": "workspace:*",
    "@codebuff/common": "workspace:*",
    "@codebuff/internal": "workspace:*",
    "@codebuff/sdk": "workspace:*",
    "drizzle-orm": "^0.43.1",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@types/bun": "^1.3.5",
    "@types/lodash": "^4.17.21",
    "@types/node": "22"
  }
}
